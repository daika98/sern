<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Enter users</title>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css">
    
    <!-- jQuery library -->
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.3/dist/jquery.slim.min.js"></script>
    
    <!-- Popper JS -->
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
    
    <!-- Latest compiled JavaScript -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>
    <div class="container">
        <div class="col-10">
            <div class="d-flex justify-content-center mt-3">
                <h3>List of datas</h3>
            </div>
            <table class="table table-hover table-bordered mt-3">
                <thead>
                    <tr>
                        <th>Firstname</th>
                        <th>Lastname</th>
                        <th>Email</th>
                        <th >Address</th>
                        
                    </tr>
                </thead>
                <tbody>
                    <% for(var i=0; i < tableData.length; i++) { %>
                        <tr>
                            <td>
                                <%= tableData[i].firstName %>
                            </td>
                            <td>
                                <%= tableData[i].lastName %>
                            </td>
                            <td>
                                <%= tableData[i].email %>
                            </td>
                            <td>
                                <%= tableData[i].address %>
                            </td>
                            <td>
                            <a href="/edit-crud?id=<%= tableData[i].id %>" class="text-center text-primary">Edit</a>
                            </td>
                            <td>
                            <a href="/delete-crud?id=<%= tableData[i].id %>" class="text-center text-danger">Delete</a>
                            </td>
                        </tr>
                        <% } %>
                </tbody>
            </table>
                    </div>
                </div>
</body>
</html>